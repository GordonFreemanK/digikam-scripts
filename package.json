{
  "devDependencies": {
    "@semantic-release/github": "^8.0.2",
    "semantic-release": "^19.0.2"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "path": "pub/**",
              "label": "GFK.Image"
            }
          ]
        }
      ]
    ]
  }
}